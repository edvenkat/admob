<?xml version='1.0' encoding='utf-8'?>
<widget id="hello.admob.venkat" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello AdMob</name>
    <description>
        A sample cordova-plugin-admob-free application.
    </description>
    <author email="ratson@users.noreply.github.com" href="https://github.com/ratson/cordova-plugin-admob-free">
        Ratson
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="6.4.0" />
    <engine name="ios" spec="~4.5.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-admob-free" spec="~0.12.0" />
   
    <!-- <plugin name="cordova-plugin-firebase-crashlytics" source="npm">
		<param name="ANDROID_FIREBASE_CORE_VERSION" value="16.0.0" />
	</plugin> -->
	<plugin spec="https://github.com/edvenkat/cordova-plugin-firebase-crashlytics">
	</plugin> 
</widget>
